def convert_1b(x, LUT):
    y = []
    for i in range(len(x)):
        pos = int(x[i] + 8)
        tmp = [-1 if val==0 else 1 for val in LUT[-1-pos]]
        y = np.concatenate((y, tmp))
    return np.array(y)

def paralelToSerialConverter(x, LUT):
    y = []

    for i in range(len(x)):
        pos = int(x[i] + 8)
        y = np.concatenate((y, LUT[-1-pos]))
    return np.array(y)

