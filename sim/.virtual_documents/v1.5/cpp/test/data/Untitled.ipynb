def quant(x, typed="mid-tread"):
    y = []
    for i in range(len(x)):
        x_tmp = x[i] - int(x[i])
        if typed == "mid-tread":
            if x_tmp > -0.5 and x_tmp < 0.5:
                y_new = int(x[i])
            else:
                if x[i] > 0:
                    y_new = int(x[i]) + 1
                else:
                    y_new = int(x[i]) - 1
        elif typed == "mid-rise":
            if x[i] > 0:
                y_new = int(x[i])
            else:
                y_new = int(x[i]) - 1
        y.append(y_new)
    return np.array(y)


