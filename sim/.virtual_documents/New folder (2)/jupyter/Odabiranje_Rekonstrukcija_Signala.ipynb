def genSines(t):
    f1 = 89.0e6
    f2 = 91.0e6
    A1 = 1
    A2 = 0.1
    y = A1*np.sin(2*np.pi*f1*t) + A2*np.sin(2*np.pi*f2*t)
    y *= win
    return y

