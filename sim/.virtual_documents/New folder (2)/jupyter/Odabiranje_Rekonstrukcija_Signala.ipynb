from scipy.signal import *
logScale = False
def fn_nrz(f):
    T = 1
    i=0
    a = np.pi*f*T
    v = np.sin(a+1e-15)/(a+1e-15)
    if logScale:
        g = 20*np.log10(abs(v))
    else:
        g = abs(v)
    return g
